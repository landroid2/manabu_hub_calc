{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/webhook",
            "methods": ["POST"],
            "dest": "index.js"
        },
        {
            "src": "/",
            "methods": ["GET"],
            "dest": "index.js"
        }
    ]
}